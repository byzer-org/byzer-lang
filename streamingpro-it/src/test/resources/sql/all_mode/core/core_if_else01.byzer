--%comparator=tech.mlsql.it.IgnoreResultComparator

select 1 as a as b;
set wow=`select count(*) from b ` where type="sql" and mode="runtime";

!if '''select :wow as :newwow; :newwow > 11 ''';

    !println "yes";
    select * from b as final_table;
!else;
    !println "no";
    select 2 as a from b as final_table;
!fi;

select * from final_table as result;

!assert result ''':a == 2''' "Failed to execute if-else statement!";