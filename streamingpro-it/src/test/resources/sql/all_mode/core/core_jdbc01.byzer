--%comparator=tech.mlsql.it.IgnoreResultComparator

--MySQL
set user = "root";
set password = "root";
set jdbc_yrl = "jdbc:mysql://byzer-it-mysql:3306/mysql?characterEncoding=utf8&zeroDateTimeBehavior=convertToNull&tinyInt1isBit=false&useSSL=false";

connect jdbc
where
    url="${jdbc_yrl}"
    and driver="com.mysql.jdbc.Driver"
    and user="${user}"
    and password="${password}"
as db_mysql;

load jdbc.`db_mysql.db` as crawler_table;

!assert crawler_table '''1 == 1''' "crawler_table";